{
    "name": "exs/google-oauth-invite-bundle",
    "type": "symfony-bundle",
    "description": "An extension of HWIO for authenticating with a registered Google account email. Any other accounts will receive an AccessDenied exception",
    "keywords": ["error", "logging", "bundle", "exception", "command", "lock", "crontab", "concurrent"],
    "homepage": "https://github.com/ExSituMarketing/EXS-GoogleOauthInviteBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Weiss",
            "email": "charles@ex-situ.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "symfony/swiftmailer-bundle": "~2.3",
        "hwi/oauth-bundle": "^0.4.0"
    },
    "require-dev": {
        "exs/google-oauth-invite-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "EXS\\GoogleOauthInviteBundle": ""
        }
    },
    "minimum-stability": "dev",
    "target-dir": "EXS/GoogleOauthInviteBundle"
}
